#include<stdio.h>

void  main()
{
        int a;
        int rem,sum=0,product=1;
        scanf("%d",&a);
        while(a>0)
        {   rem=a%10;
             sum=sum+rem;
             product=product*rem;
        a=a/10;
        }
        printf("%d\n",sum);
        printf("%d",product);
    }

    
