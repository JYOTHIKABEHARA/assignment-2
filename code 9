// Online C compiler to run C program online
#include <stdio.h>

int main()
{
    int a[100],i,n,flag,j;
    printf("enter your range ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    { 
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {   if(a[i]==1)
        { 
          
        }
        else if(a[i]==2)
        
        {
            printf("%d is a prime \n",a[i]);
        }
        else 
        {
        for(j=2;j<=n;j++)
        { flag=0;
        
        if(a[i]%j==0)
        
        flag=1;
        break;
        }
        if(flag==0)
        {
            printf("%d is a prime\n",a[i]);

        }
}

    }  
    return 0;
}
